                  </AnchorPane>
               </center>
               <top>
                  <AnchorPane prefHeight="45.0" prefWidth="450.0" style="-fx-background-color: #ffffff;" BorderPane.alignment="CENTER">
                     <children>
                        <Label fx:id="user_id" layoutX="8.0" layoutY="7.0" prefHeight="31.0" prefWidth="434.0">
                           <font>
                              <Font name="Arial" size="12.0" />
                           </font></Label>
                     </children></AnchorPane>
               </top>
            </BorderPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
