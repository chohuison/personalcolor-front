               </children></AnchorPane>
         </top>
         <bottom>
            <ScrollPane prefHeight="211.0" prefWidth="450.0" BorderPane.alignment="CENTER">
               <content>
                  <TextFlow prefHeight="288.0" prefWidth="432.0">
                     <children>
                        <TextArea fx:id="enrollText" prefHeight="199.0" prefWidth="432.0" />
                     </children></TextFlow>
               </content>
            </ScrollPane>
         </bottom>
         <center>
            <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <center>
                  <AnchorPane prefHeight="450.0" prefWidth="450.0" BorderPane.alignment="CENTER">
                     <ImageView fx:id="imgView" fitHeight="450.0" fitWidth="450.0" pickOnBounds="true" preserveRatio="false" />
